        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH first_booking AS ( \r\n  SELECT\r\n    u.id AS user_id,\r\n    date_trunc('month', u.\"createdAt\") AS reg_month,\r\n    EXTRACT(EPOCH FROM (MIN(b.\"createdAt\") - u.\"createdAt\")) / 86400 AS days_to_first_booking\r\n  FROM \"User\" u\r\n  JOIN \"Booking\" b ON b.\"userId\" = u.id\r\n  GROUP BY u.id, reg_month\r\n)\r\nSELECT\r\n  reg_month AS \"time\",\r\n  ROUND(AVG(days_to_first_booking),1) AS value\r\nFROM first_booking\r\nGROUP BY reg_month\r\nORDER BY reg_month;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
