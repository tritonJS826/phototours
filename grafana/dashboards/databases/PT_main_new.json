        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH per_day AS (\r\n  SELECT\r\n    date_trunc('day', \"createdAt\") AS day,\r\n    COUNT(*) AS bookings,                                        -- все заявки (резервы)\r\n    COUNT(*) FILTER (WHERE status = 'CANCELLED') AS cancelled    -- отмены\r\n  FROM \"Booking\"\r\n  GROUP BY 1\r\n)\r\nSELECT\r\n  day AS \"time\",\r\n  bookings       AS reservations,                                -- ряд 1: резервы\r\n  cancelled,                                                      -- ряд 2: отмены\r\n  CASE                                                          -- ряд 3: рейтинг (успех)\r\n    WHEN bookings > 0 THEN ROUND(100.0 * (bookings - cancelled) / bookings, 2)\r\n    ELSE NULL\r\n  END AS rating_pct\r\nFROM per_day\r\nORDER BY 1;\r\n",
          "refId": "A",
